                // $('#question-subject').attr('value','-1');
                $('.init').prop('disabled', false);
                $('.init').prop('selected', false);
                $('.init').prop('selected', true);
                $('.init').prop('disabled', true);
                $('#question-content').val('');
      				},
      				error: function () {
      					alert('전송에 실패했습니다.');
                $('.question-send').attr('disabled', false);
      				}
      			});
          });


